{
  "name": "dockery",
  "version": "1.1.0",
  "description": "Build your package and create docker image",
  "keywords": [
    "docker",
    "automation",
    "containerize",
    "container",
    "continuous-integration"
  ],
  "docker-repo": "non-repo",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "bin": {
    "dockery": "dist/index.js"
  },
  "repository": "git@github.com:lumio/dockery.git",
  "author": "Benjamin Falk <benjamin.falk@cyanjoe.com>",
  "license": "MIT",
  "scripts": {
    "start": "ts-node index.ts",
    "test": "jest",
    "lint": "tslint -p . -t stylish \"src/**/*.ts\"",
    "build": "(rm -rf dist; tsc -p tsconfig.prod.json) && chmod +x dist/index.js",
    "prepublishOnly": "npm run test && npm run build"
  },
  "dependencies": {
    "kleur": "3.0.2",
    "minimist": "1.2.6"
  },
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "@types/minimist": "^1.2.0",
    "@types/node": "^11.11.6",
    "jest": "^24.5.0",
    "ts-jest": "^24.0.0",
    "ts-node": "8.0.3",
    "tslint": "5.14.0",
    "tslint-config-airbnb": "^5.11.1",
    "typescript": "3.3.4000"
  },
  "jest": {
    "preset": "ts-jest",
    "collectCoverage": true,
    "coverageDirectory": "coverage"
  }
}
